*.color2:       #87875f
*.color10:      #A1A179

! yellow
*.color3:       #af875f
*.color11:      #C9A179

! blue
*.color4:       #878787
*.color12:      #A1A1A1

! magenta
*.color5:       #af8787
*.color13:      #C9A1A1

! cyan
*.color6:       #87afaf
*.color14:      #A1C9C9

! white
